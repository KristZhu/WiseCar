        <activity android:name="com.wisecarCompany.wisecarapp.user.login.LoginActivity" />
        <activity android:name="com.wisecarCompany.wisecarapp.user.create.CreateUserActivity" />
        <activity android:name="com.wisecarCompany.wisecarapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
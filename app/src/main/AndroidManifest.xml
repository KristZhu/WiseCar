        <activity android:name="com.wisecarCompany.wisecarapp.user.vehicle.EditVehicleActivity" />
        <activity android:name="com.wisecarCompany.wisecarapp.user.vehicle.VehicleActivity" />
        <activity android:name="com.wisecarCompany.wisecarapp.user.vehicle.AddVehicleActivity" />
